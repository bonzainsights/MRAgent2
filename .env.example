# MRAgent API Keys
# Copy this file to .env and fill in your keys
# Get free NVIDIA API keys at: https://build.nvidia.com

# NVIDIA APIs
PORT=16226
NVIDIA_BASE_URL=https://integrate.api.nvidia.com/v1

# Paste your NVIDIA API key here to unlock all models instantly
NVIDIA_API_KEY=nvapi-xxxxx

# Groq
GROQ_API_KEY=xxxxx

# Search Provider (brave, google, langsearch)
SEARCH_PROVIDER=brave

# Search
# Brave
BRAVE_SEARCH_API_KEY=your_brave_search_api_key
# Google
GOOGLE_SEARCH_API_KEY=your_api_key_here
GOOGLE_SEARCH_CSE_ID=your_search_engine_id_here
# langsearch
LANGSEARCH_API_KEY=sk-xxxxxx

# Image Provider (flux = default, google = Google AI Studio)
IMAGE_PROVIDER=flux

# Google AI Studio (optional — set IMAGE_PROVIDER=google to use)
GOOGLE_AI_STUDIO_KEY=your_google_ai_studio_key

# Identity
USER_NAME=User
AGENT_NAME=MRAgent

# Autonomy Trust Level (cautious, balanced, autonomous)
TRUST_LEVEL=balanced

# Web UI Authentication (optional — leave empty for no auth)
# MRAGENT_ACCESS_TOKEN=your_secret_token

# Skills
# AgentMail (get keys at agentmail.to)
AGENTMAIL_API_KEY=your_agentmail_api_key

# Telegram (get token from @BotFather)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
ALLOWED_TELEGRAM_CHATS=your_telegram_chat_id # Optional default target

# OpenAI (optional, for comparison/backup)
# OPENAI_API_KEY=

# DeepSeek (free official API — get key at platform.deepseek.com)
# deepseek-chat (V3) and deepseek-reasoner (R1) are both available free
DEEPSEEK_API_KEY=sk-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx